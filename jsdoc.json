{
  "source": {
    "include": [
      "server",
      "server/validation",
      "server/routes",
      "server/models",
      "server/middleware",
      "client",
      "client/src",
      "client/src/Shared",
      "client/src/Shared/Logo",
      "client/src/Shared/Modal",
      "client/src/Shared/noteType",
      "client/src/Pages",
      "client/src/Pages/SharedComponents",
      "client/src/Hooks",
      "client/src/Context",
      "client/src/NoteComponents",
      "client/src/NoteComponents/palette"
    ],
    "includePattern": ".js$",
    "excludePattern": "(docs|node_modules/|client/node_modules/|client/build/)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": false,
    "destination": "./docs/",
    "readme": "./readme.md"
  }
}