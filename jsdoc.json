{
  "source": {
    "include": [
      "",
      "validation",
      "routes",
      "models",
      "middleware",
      "client",
      "client/src",
      "client/src/Shared",
      "client/src/Shared/Logo",
      "client/src/Shared/Modal",
      "client/src/Pages",
      "client/src/Pages/SharedComponents",
      "client/src/Hooks",
      "client/src/Context",
      "client/src/Notes",
      "client/src/Notes/palette",
      "client/src/Notes/noteType"
    ],
    "includePattern": ".js$",
    "excludePattern": "(docs|node_modules/|client/node_modules/|client/build/)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": false,
    "destination": "./docs/",
    "readme": "./readme.md"
  }
}